{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "score/{weekNumber}/{weekName}/{matchId}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "matchData",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "WeekData",
      "createIfNotExists": false,
      "collectionName": "MatchEvents",
      "id": "{weekName}",
      "partitionKey": "{weekNumber}",
      "connectionStringSetting": "CosmosDBConnection"
    },
    {
      "name": "matchDataUpdated",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "WeekData",
      "createIfNotExists": false,
      "collectionName": "MatchEvents",
      "connectionStringSetting": "CosmosDBConnection"
    },
    {
      "name": "teamPools",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "WeekData",
      "collectionName": "TeamPool",
      "id": "{weekName}",
      "partitionKey": "{weekNumber}",
      "connectionStringSetting": "CosmosDBConnection"
    },

    {
      "name": "teamPoolsUpdate",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "WeekData",
      "collectionName": "TeamPool",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}
