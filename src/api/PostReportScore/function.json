{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "modify/{weekId}/match/{matchId}/score"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "matchData",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "desto",
      "createIfNotExists": false,
      "collectionName": "matchdata",
      "id": "{weekId}",
      "partitionKey": "{weekId}",
      "connectionStringSetting": "CosmosDBConnection"
    },
    {
      "name": "matchDataUpdated",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "desto",
      "createIfNotExists": false,
      "collectionName": "matchdata",
      "connectionStringSetting": "CosmosDBConnection"
    },
    {
      "name": "teamPools",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "desto",
      "collectionName": "teampool",
      "id": "{weekId}",
      "partitionKey": "{weekId}",
      "connectionStringSetting": "CosmosDBConnection"
    },

    {
      "name": "teamPoolsUpdate",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "desto",
      "collectionName": "teampool",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}
