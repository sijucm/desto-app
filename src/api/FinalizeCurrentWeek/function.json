{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "teampools",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "WeekData",
      "collectionName": "TeamPool",
      "id": "{weekName}",
      "partitionKey": "{weekNumber}",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}
