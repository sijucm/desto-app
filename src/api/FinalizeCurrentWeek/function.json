{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "gameschedule/{id}/close"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "currentTeamPools",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "desto",
      "collectionName": "teampool",
      "id": "{id}",
      "partitionKey": "{id}",
      "connectionStringSetting": "CosmosDBConnection"
    },

    {
      "name": "currentTeamPoolsUpdated",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "desto",
      "collectionName": "teampool",
      "id": "{id}",
      "partitionKey": "{id}",
      "connectionStringSetting": "CosmosDBConnection"
    },

    {
      "name": "newTeamPools",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "desto",
      "collectionName": "teampool",
      "connectionStringSetting": "CosmosDBConnection"
    },
    {
      "name": "newMatchData",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "desto",
      "collectionName": "matchdata",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}
