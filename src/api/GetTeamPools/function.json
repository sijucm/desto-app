{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "teamPools/{partitionKeyValue}/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "teampools",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "WeekData",
      "collectionName": "TeamPool",
      "id": "{id}",
      "partitionKey": "{partitionKeyValue}",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}
