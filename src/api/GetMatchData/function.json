{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
      ],
      "route": "matches/{id}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "matchData",
      "type": "cosmosDB",
      "direction": "in",
      "databaseName": "desto",
      "createIfNotExists": false,
      "collectionName": "matchdata",
      "id": "{id}",
      "partitionKey": "{id}",
      "connectionStringSetting": "CosmosDBConnection"
    }
  ]
}
