{
  "routes": [
    {
      "route": "/api/score",
      "allowedRoles": ["teamadmin", "destoadmin", "futadmin", "siteadmin"]
    },
    {
      "route": "/login",
      "serve": "/.auth/login/aad"
    },

    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }

  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/custom-404.html"
    },
    {
      "errorType": "Unauthenticated",
      "statusCode": "302",
      "serve": "/login"
    }
  ]
}
